{
  "name": "dpc-sdp/tide_publication",
  "description": "Publication content type and related configuration for Tide Drupal 8 distribution",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "require": {
    "dpc-sdp/tide_core": "^2.0.0",
    "dpc-sdp/tide_landing_page": "^2.0.0",
    "drupal/entity_hierarchy": "~2.0"
  },
  "suggest": {
    "dpc-sdp/tide_api:^1.2.6": "Allows to use Drupal in headless mode"
   },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patches": {
      "dpc-sdp/tide_core": {
        "Moved Topic and Tags field to node form sidebar - https://github.com/dpc-sdp/tide_core/pull/213": "https://github.com/dpc-sdp/tide_core/pull/213.diff"
      },
      "dpc-sdp/tide_landing_page": {
        "Updated UI of Landing page node form - https://github.com/dpc-sdp/tide_landing_page/pull/124": "https://github.com/dpc-sdp/tide_landing_page/pull/124.diff"
      }
    }
  }
}
